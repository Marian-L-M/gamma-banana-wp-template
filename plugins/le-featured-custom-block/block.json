{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "theme-custom-blocks/le-featured-block",
    "title": "Le Quiz Block",
    "attributes": {
        "featuredId": {
            "type": "number"
        },
        "bgColor": {
            "type": "string",
            "default": "#ebebeb"
        }
    },
    "editorStyle": "file:./build/index.css",
    "editorScript": "file:./build/index.js",
    "script": [ "file:./build/script.css", "frontend-shared-script" ],
    "style": [ "file:./build/style.css", "frontend-shared-style" ],
    "viewScript": [ "file:./build/frontend.js", "frontend-shared-view-script" ],
    "viewStyle": [ "file:./build/frontend.css", "frontend-view-style" ]
}